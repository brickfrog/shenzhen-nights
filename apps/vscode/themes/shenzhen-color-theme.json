    "panel.background": "#101820",
    "panel.border": "#485058",
    "panelTitle.activeBorder": "#00cc99",
    "panelTitle.activeForeground": "#e5e9f0",
    "panelTitle.inactiveForeground": "#b8c2cc",
    "terminal.background": "#0a0f14",
    "terminal.foreground": "#d8dce0",
    "terminal.ansiBlack": "#101820",
    "terminal.ansiRed": "#f04e98",
    "terminal.ansiGreen": "#00cc99",
    "terminal.ansiYellow": "#ffb86c",
    "terminal.ansiBlue": "#50a0ff",
    "terminal.ansiMagenta": "#b07eff",
    "terminal.ansiCyan": "#00ccff",
    "terminal.ansiWhite": "#b8c2cc",
