    "inputValidation.infoBorder": "#50a0ff",
    "inputValidation.warningBorder": "#ffb86c",
    "inputValidation.errorBorder": "#f04e98",
    "dropdown.background": "#101820",
    "dropdown.border": "#485058",
    "button.background": "#50a0ff",
    "button.foreground": "#0a0f14",
