    "panelTitle.inactiveForeground": "#b8c2cc",
    "terminal.background": "#0a0f14",
    "terminal.foreground": "#d8dce0",
    "terminal.ansiBlack": "#101820",
    "terminal.ansiRed": "#FF3366",
    "terminal.ansiGreen": "#00CC99",
    "terminal.ansiYellow": "#FFCC00",
    "terminal.ansiBlue": "#1A8CFF",
    "terminal.ansiMagenta": "#CC33FF",
    "terminal.ansiCyan": "#00CCFF",
    "terminal.ansiWhite": "#b8c2cc",
    "terminal.ansiBrightBlack": "#485058",
    "terminal.ansiBrightRed": "#FF5C8A",
    "terminal.ansiBrightGreen": "#20e0a0",
    "terminal.ansiBrightYellow": "#FFDA3B",
    "terminal.ansiBrightBlue": "#57A5FF",
    "terminal.ansiBrightMagenta": "#D966FF",
    "terminal.ansiBrightCyan": "#40e0ff",
    "terminal.ansiBrightWhite": "#e5e9f0",
    "scrollbarSlider.background": "#1A8CFF40",
    "scrollbarSlider.hoverBackground": "#1A8CFF60",
    "scrollbarSlider.activeBackground": "#1A8CFF80",
    "badge.background": "#00cc99",
    "badge.foreground": "#0a0f14",
    "progressBar.background": "#1A8CFF",
