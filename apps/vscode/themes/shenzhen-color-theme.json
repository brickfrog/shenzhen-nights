    "peekViewEditor.background": "#101820",
    "peekViewResult.background": "#101820",
    "peekViewTitle.background": "#101820"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#485058",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#00cc99"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#b07eff"
      }
    },
    {
      "scope": ["constant.language", "constant.character.escape"],
      "settings": {
        "foreground": "#b07eff"
      }
    },
    {
      "scope": ["constant.regexp", "string.regexp"],
      "settings": {
        "foreground": "#f04e98"
      }
    },
    {
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#00ccff"
      }
    },
    {
      "scope": "constant.other.color",
      "settings": {
        "foreground": "#00ccff"
      }
    },
    {
      "scope": ["variable", "support.variable", "punctuation.definition.variable"],
      "settings": {
        "foreground": "#d8dce0"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#ffb86c",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#b07eff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "punctuation.definition.keyword",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "support.type.primitive",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#50a0ff"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#00ccff"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.property-value",
        "support.constant.font-name",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#ffca88"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "support.type",
        "meta.type.declaration"
      ],
      "settings": {
        "foreground": "#ffb86c",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.namespace", "entity.name.section"],
      "settings": {
        "foreground": "#ffb86c",
         "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#f04e98"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#b07eff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#b07eff"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#b8c2cc"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#ffffff",
        "background": "#f04e98"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#ffffff",
        "background": "#b07eff"
      }
    },
    {
        "scope": "markup.bold",
        "settings": {
            "fontStyle": "bold",
            "foreground": "#ff7eb6"
        }
    },
    {
        "scope": "markup.italic",
        "settings": {
            "fontStyle": "italic",
            "foreground": "#ffb86c"
        }
    },
    {
        "scope": "markup.underline",
        "settings": {
            "fontStyle": "underline"
        }
    },
    {
        "scope": "markup.heading",
        "settings": {
            "fontStyle": "bold",
            "foreground": "#50a0ff"
        }
    },
    {
        "scope": "markup.list",
        "settings": {
            "foreground": "#d8dce0"
        }
    },
    {
        "scope": "markup.quote",
        "settings": {
            "fontStyle": "italic",
            "foreground": "#485058"
        }
    },
    {
        "scope": "markup.inline.raw",
        "settings": {
            "foreground": "#00cc99"
        }
    },
    {
        "scope": "markup.inserted",
        "settings": {
            "foreground": "#00cc99"
        }
    },
    {
        "scope": "markup.deleted",
        "settings": {
            "foreground": "#f04e98"
        }
    },
    {
        "scope": "markup.changed",
        "settings": {
            "foreground": "#b07eff"
        }
    }
  ]
} 