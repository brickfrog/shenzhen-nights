    "inputValidation.infoBorder": "#1A8CFF",
    "inputValidation.warningBorder": "#FFCC00",
    "inputValidation.errorBorder": "#FF3366",
    "dropdown.background": "#101820",
    "dropdown.border": "#485058",
    "button.background": "#1A8CFF",
    "button.foreground": "#0a0f14",
