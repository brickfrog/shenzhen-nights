    "list.focusBackground": "#00cc9940",
    "list.highlightForeground": "#00cc99",
    "statusBar.background": "#0a0f14",
    "statusBar.foreground": "#b8c2cc",
    "statusBar.noFolderBackground": "#101820",
    "statusBarItem.remoteBackground": "#1A8CFF",
    "statusBarItem.remoteForeground": "#0a0f14",
    "titleBar.activeBackground": "#0a0f14",
    "titleBar.activeForeground": "#d8dce0",
    "titleBar.inactiveBackground": "#0a0f14",
