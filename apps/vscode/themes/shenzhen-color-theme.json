    "peekViewEditor.background": "#101820",
    "peekViewResult.background": "#101820",
    "peekViewTitle.background": "#101820"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#485058",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#00cc99"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#CC33FF"
      }
    },
    {
      "scope": ["constant.language", "constant.character.escape"],
      "settings": {
        "foreground": "#CC33FF"
      }
    },
    {
      "scope": ["constant.regexp", "string.regexp"],
      "settings": {
        "foreground": "#FF3366"
      }
    },
    {
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#00CCFF"
      }
    },
    {
      "scope": "constant.other.color",
      "settings": {
        "foreground": "#00CCFF"
      }
    },
    {
      "scope": ["variable", "support.variable", "punctuation.definition.variable"],
      "settings": {
        "foreground": "#d8dce0"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#FFCC00",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#CC33FF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "punctuation.definition.keyword",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "support.type.primitive",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#1A8CFF"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#00CCFF"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.property-value",
        "support.constant.font-name",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#FFDA3B"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "support.type",
        "meta.type.declaration"
      ],
      "settings": {
        "foreground": "#FFCC00",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.namespace", "entity.name.section"],
      "settings": {
        "foreground": "#FFCC00",
         "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#FF3366"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#CC33FF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#CC33FF"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#b8c2cc"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#ffffff",
        "background": "#FF3366"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#ffffff",
        "background": "#CC33FF"
      }
    },
    {
        "scope": "markup.bold",
        "settings": {
            "fontStyle": "bold",
            "foreground": "#FFCC00"
        }
    },
    {
        "scope": "markup.italic",
        "settings": {
            "fontStyle": "italic",
            "foreground": "#CC33FF"
        }
    },
    {
        "scope": "markup.underline",
        "settings": {
            "fontStyle": "underline"
        }
    },
    {
        "scope": "markup.heading",
        "settings": {
            "fontStyle": "bold",
            "foreground": "#FFCC00"
        }
    },
    {
        "scope": "markup.list",
        "settings": {
            "foreground": "#1A8CFF"
        }
    },
    {
        "scope": "markup.quote",
        "settings": {
            "fontStyle": "italic",
            "foreground": "#485058"
        }
    },
    {
        "scope": "markup.inline.raw",
        "settings": {
            "foreground": "#00cc99"
        }
    },
    {
        "scope": "markup.inserted",
        "settings": {
            "foreground": "#00CC99"
        }
    },
    {
        "scope": "markup.deleted",
        "settings": {
            "foreground": "#FF3366"
        }
    },
    {
        "scope": "markup.changed",
        "settings": {
            "foreground": "#1A8CFF"
        }
    },
    {
      "scope": "markup.link",
      "settings": {
        "foreground": "#00CCFF",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.raw",
      "settings": {
        "foreground": "#00CC99"
      }
    }
  ]
} 